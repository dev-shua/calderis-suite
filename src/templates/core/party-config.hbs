<section class="cs-dock-config-body">
  <div class="cols">
    <div class="col">
      <h4>{{localize "CS.DMDock.Config.Visible"}}</h4>
      <ul class="list chosen">
        {{#each chosen}}
          <li data-key="{{this.key}}">
            <span class="label">{{this.label}}</span>
            <div class="actions">
              {{! <button type="button" data-action="move" data-dir="up"><i
                  class="fas fa-arrow-up"
                ></i></button>
              <button type="button" data-action="move" data-dir="down"><i
                  class="fas fa-arrow-down"
                ></i></button> }}
              <button type="button" data-action="toggle"><i class="fas fa-eye-slash"></i></button>
            </div>
          </li>
        {{/each}}
      </ul>
    </div>
    <div class="col">
      <h4>{{localize "CS.DMDock.Config.Hidden"}}</h4>
      <ul class="list others">
        {{#each others}}
          <li data-key="{{this.key}}">
            <span class="label">{{this.label}}</span>
            <div class="actions">
              <button type="button" data-action="toggle"><i class="fas fa-eye"></i></button>
            </div>
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
  <footer class="dialog-buttons">
    <button type="button" data-action="save" class="default">{{localize "Save"}}</button>
  </footer>
</section>